     <COMMAND name="setspeed"
             help="Set slot info for system">
        <PARAM name="name"
                   help="port id"
                   ptype="FPORT"/>
        <PARAM name="sn"
               help="Set portspeed"
              ptype="TIANSPEED"/>
        <ACTION builtin="shell_otu_setspeed">${name} ${sn}</ACTION>
    </COMMAND> 	
    
     <COMMAND name="setloopback"
             help="Set slot info for system">
        <PARAM name="name"
                   help="port id"
                   ptype="FPORT"/>
        <PARAM name="ttt"
               help="Set loopback"
              ptype="NSTATE"/>
        <ACTION builtin="shell_otu_setloopback">${name} ${ttt}</ACTION>
    </COMMAND> 	
